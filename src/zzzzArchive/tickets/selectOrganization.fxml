<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<HBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="helpDeskTeam.tickets.selectedOrganizationController">
   <children>
        <VBox>
            <children>
                <HBox>
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Which Organization">
                            <font>
                                <Font size="24.0" />
                            </font>
                            <HBox.margin>
                                <Insets left="15.0" right="15.0" top="30.0" />
                            </HBox.margin>
                        </Text>
                        <ComboBox promptText="Organization" style="-fx-background-color: FC766AFF; -fx-background-radius: 15;; -fx-cursor: hand">
                            <HBox.margin>
                                <Insets left="15.0" right="15.0" top="30.0" />
                            </HBox.margin>
                        </ComboBox>
                        <VBox>
                            <HBox.margin>
                                <Insets left="15.0" right="15.0" top="30.0" />
                            </HBox.margin>
                        </VBox>
                        <Button mnemonicParsing="false" styleClass="Button" stylesheets="/css/helpspot.css" text="Select" onAction="#loadOrganizationOldTickets">
                            <HBox.margin>
                                <Insets left="15.0" right="15.0" top="30.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <Button mnemonicParsing="false" styleClass="Button" stylesheets="/css/helpspot.css" text="Close" textFill="WHITE" onAction="#close" fx:id="closeButton">
                    <VBox.margin>
                        <Insets bottom="15.0" left="15.0" top="30.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
   </children>
</HBox>
